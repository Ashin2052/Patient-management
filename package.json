{
  "name": "monorepo-example",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "lerna": "^5.3.0"
  },
  "scripts": {
    "run:fe": "lerna run --scope=client/client start",
    "run:be": "lerna run --scope=server/server run",
    "run:both": "lerna run --parallel start"
  }
}